{
   "manifest_version": 2,
   "content_scripts": [ {
      "exclude_globs": [  ],
      "exclude_matches": [  ],
      "include_globs": [  ],
      "js": [ "next-step-for-trello.user.js" ],
      "css": [ "next-step-for-trello.css" ],
      "matches": [ "http://trello.com/*", "https://trello.com/*" ],
      "run_at": "document_start"
   } ],
   "converted_from_user_script": true,
   "description": "Check tasks directly from your Trello boards",
   "icons": { "128": "icon-128.png" },
   "web_accessible_resources": [
     "next-step-for-trello.css",
     "/docs/assets/announcement.json"
   ],
   "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
   "name": "Next Step for Trello",
   "short_name": "Next Step",
   "version": "1.8.15"
}
